@import 'iconfont';
.container {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  position: relative;
}

.bar {
  flex: 1;
  height: 68rpx;
  /* width: 100rpx; */
  font-size: 28rpx;
  border-radius: 50px;
  background-color: #f5f5f5;
  padding-left: 60rpx;
}

.search {
  background: #00B26A;
  color: #fff;
  margin-left: 20rpx;
  font-size: 24rpx;
  width: 120rpx;
}

.in-bar {
  color: #999;
}

.find {
  position: absolute;
  top: 50%;
  transform: translate(16rpx, -50%);
  color: #9f9f9f; 
}